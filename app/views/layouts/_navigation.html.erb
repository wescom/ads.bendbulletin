<% if user_signed_in? %>
	<div class="navbar">
		<div class="navbar-inner">
		  <ul class="nav">
		    <li><%= link_to "Home", root_url %></li>
				<% if (current_user.has_role? :view) %>
	    		<li><%= link_to "My Jobs", myjobs_url %></li>
				<% else %>
	    		<li><%= link_to "All Jobs", jobs_url %></li>
				<% end %>
				<li><a> | </a></li>
				<% get_menu_tabs().each do |f| %>
			    <li><%= link_to f.tab_name, upload_type_path(f) %></li>
				<% end %>
		  </ul>
		</div>
	</div>
<% end %>